name:                agda-light
version:             0.1.0.0
-- synopsis:            
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Tim Engler
maintainer:          engler@gmail.com
-- copyright:           
-- category:            
build-type:          Simple
extra-source-files:  ChangeLog.md
cabal-version:       >=1.10

executable agda-light
  hs-source-dirs:   src/full/
  include-dirs:     src/full/
  main-is:             Main.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       
                       TCache >= 0.12.0,
                       array >= 0.5.1.1,
                       base >=4.9 && <4.10,
                       binary >= 0.8.3.0,
                       bytestring >= 0.10.8.1,
                       containers >= 0.5.7.1,
                       deepseq >= 1.4.2.0,
                       directory >= 1.3.0.0,
                       filepath >= 1.4.1.1,
                       hashable >= 1.2.6.0,
                       ieee754 >= 0.7.9,
                       monad-loops >= 0.4.3,
                       mtl >= 2.2.1,
                       parsec >= 3.1.11,
                       pretty >= 1.1.3.3,
                       regex-tdfa >= 1.2.2,
                       sorted-list >= 0.2.0.0,
                       stm >= 2.4.4.1,
                       strict >= 0.3.2,
                       text >= 1.2.2.1,
                       transformers >= 0.5.2.0,
                       unagi-chan >= 0.4.0.0,
                       unordered-containers >= 0.2.8.0
  -- hs-source-dirs:      
  default-language:    Haskell2010
